package in.com.batch;

public class ExampleSum {
public static void main (String[]args) {
	int a = 9;
	int count = 0;
	int sum = 0;
	for (int i = 100; i <200; i++) {
		if ( i%a ==0) {
//			count = i;
//			System.out.println(count);
//			sum = sum + count;
			System.out.println(i);
			sum = sum + i;
		}
	}
	System.out.println("sum = "+ sum);
}
}
